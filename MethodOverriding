class A{
    public  int add(int n1,int n2){
       return n1+n2;
    }
    public void add(int a,int b,int c){
        return a+b+c;
    }
    
}
class B extends A{
   public  int add(int n1,int n2){
       return n1+n2+1;
    }
}
public class MethodOverRiding{
    public static void main(String args[]){
        B obj=new B();
        obj.add(4,5);
    }
}
